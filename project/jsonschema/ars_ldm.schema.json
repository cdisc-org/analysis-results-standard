{
    "$defs": {
        "Analysis": {
            "additionalProperties": false,
            "description": "An analysis that is designed to meet a requirement of the reporting event. Each analysis is defined as a set of specifications, including:\n- The analysis variable that is the subject of the analysis.\n- The analysis method (set of statistical operations) that is performed for the analysis variable.\n- The analysis set (subject population) for which the analysis is performed.\n- The subset of data records on which the analysis is performed (optional).\n- One or more factors by which either subjects or data records are grouped for analysis (optional).",
            "properties": {
                "analysisSetId": {
                    "type": "string"
                },
                "categoryIds": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                },
                "dataSubsetId": {
                    "type": "string"
                },
                "dataset": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                },
                "documentRefs": {
                    "items": {
                        "$ref": "#/$defs/DocumentRef"
                    },
                    "type": "array"
                },
                "id": {
                    "type": "string"
                },
                "methodId": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "orderedGroupings": {
                    "items": {
                        "$ref": "#/$defs/OrderedGroupingFactor"
                    },
                    "type": "array"
                },
                "purpose": {
                    "anyOf": [
                        {
                            "$ref": "#/$defs/AnalysisPurpose"
                        },
                        {
                            "type": "string"
                        }
                    ],
                    "description": "The purpose of the analysis within the body of evidence (e.g., section in the clinical study report)."
                },
                "reason": {
                    "anyOf": [
                        {
                            "$ref": "#/$defs/AnalysisReason"
                        },
                        {
                            "type": "string"
                        }
                    ],
                    "description": "The rationale for performing this analysis. It indicates when the analysis was planned."
                },
                "referencedAnalysisOperations": {
                    "items": {
                        "$ref": "#/$defs/ReferencedAnalysisOperation"
                    },
                    "type": "array"
                },
                "results": {
                    "items": {
                        "$ref": "#/$defs/OperationResult"
                    },
                    "type": "array"
                },
                "variable": {
                    "type": "string"
                },
                "version": {
                    "type": "integer"
                }
            },
            "required": [
                "id",
                "reason",
                "purpose",
                "methodId",
                "name"
            ],
            "title": "Analysis",
            "type": "object"
        },
        "AnalysisCategorization": {
            "additionalProperties": false,
            "description": "A set of related implementer-defined categories that can be used to categorize analyses or outputs.",
            "properties": {
                "categories": {
                    "items": {
                        "$ref": "#/$defs/AnalysisCategory"
                    },
                    "type": "array"
                },
                "id": {
                    "type": "string"
                },
                "label": {
                    "type": "string"
                }
            },
            "required": [
                "id",
                "categories"
            ],
            "title": "AnalysisCategorization",
            "type": "object"
        },
        "AnalysisCategory": {
            "additionalProperties": false,
            "description": "An implementer-defined category of analyses/outputs, which may include one or more sub-categorization.",
            "properties": {
                "id": {
                    "type": "string"
                },
                "label": {
                    "type": "string"
                },
                "subCategorizations": {
                    "items": {
                        "$ref": "#/$defs/AnalysisCategorization"
                    },
                    "type": "array"
                }
            },
            "required": [
                "id"
            ],
            "title": "AnalysisCategory",
            "type": "object"
        },
        "AnalysisGroup": {
            "additionalProperties": false,
            "description": "A subdivision of the subject population based on a defined factor (e.g., subjects whose treatment is Drug A, subjects whose gender is male).",
            "properties": {
                "compoundExpression": {
                    "$ref": "#/$defs/CompoundGroupExpression"
                },
                "condition": {
                    "$ref": "#/$defs/WhereClauseCondition"
                },
                "id": {
                    "type": "string"
                },
                "label": {
                    "type": "string"
                },
                "level": {
                    "type": "integer"
                },
                "order": {
                    "type": "integer"
                }
            },
            "required": [
                "id"
            ],
            "title": "AnalysisGroup",
            "type": "object"
        },
        "AnalysisMethod": {
            "additionalProperties": false,
            "description": "A set of one or more statistical operations.",
            "properties": {
                "codeTemplate": {
                    "$ref": "#/$defs/ProgrammingCodeTemplate",
                    "description": "Template programming statements and/or a reference to the template program used to perform the specific analysis."
                },
                "description": {
                    "type": "string"
                },
                "id": {
                    "type": "string"
                },
                "label": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "operations": {
                    "items": {
                        "$ref": "#/$defs/Operation"
                    },
                    "type": "array"
                }
            },
            "required": [
                "id",
                "operations",
                "name"
            ],
            "title": "AnalysisMethod",
            "type": "object"
        },
        "AnalysisPurpose": {
            "description": "The purpose of the analysis within the body of evidence (e.g., section in the clinical study report).",
            "enum": [
                "PRIMARY OUTCOME MEASURE",
                "SECONDARY OUTCOME MEASURE",
                "EXPLORATORY OUTCOME MEASURE"
            ],
            "title": "AnalysisPurpose",
            "type": "string"
        },
        "AnalysisReason": {
            "description": "The rationale for performing this analysis. It indicates when the analysis was planned.",
            "enum": [
                "SPECIFIED IN PROTOCOL",
                "SPECIFIED IN SAP",
                "DATA DRIVEN",
                "REQUESTED BY REGULATORY AGENCY"
            ],
            "title": "AnalysisReason",
            "type": "string"
        },
        "AnalysisSet": {
            "additionalProperties": false,
            "description": "A set of subjects whose data are to be included in the main analyses. This should be defined in the statistical section of the protocol. NOTE: There are a number of potential analysis sets, including, for example, the set based upon the intent-to-treat principle. [ICH E9]",
            "properties": {
                "compoundExpression": {
                    "$ref": "#/$defs/CompoundSetExpression"
                },
                "condition": {
                    "$ref": "#/$defs/WhereClauseCondition"
                },
                "id": {
                    "type": "string"
                },
                "label": {
                    "type": "string"
                },
                "level": {
                    "type": "integer"
                },
                "order": {
                    "type": "integer"
                }
            },
            "required": [
                "id"
            ],
            "title": "AnalysisSet",
            "type": "object"
        },
        "CodeParameter": {
            "additionalProperties": false,
            "description": "",
            "properties": {
                "description": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "valueSource": {
                    "description": "A reference to the prespecified source of the value for the parameter.",
                    "type": "string"
                }
            },
            "required": [
                "name"
            ],
            "title": "CodeParameter",
            "type": "object"
        },
        "CompoundGroupExpression": {
            "additionalProperties": false,
            "description": "",
            "properties": {
                "logicalOperator": {
                    "$ref": "#/$defs/ExpressionLogicalOperator"
                },
                "whereClauses": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                }
            },
            "required": [
                "logicalOperator"
            ],
            "title": "CompoundGroupExpression",
            "type": "object"
        },
        "CompoundSetExpression": {
            "additionalProperties": false,
            "description": "",
            "properties": {
                "logicalOperator": {
                    "$ref": "#/$defs/ExpressionLogicalOperator"
                },
                "whereClauses": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                }
            },
            "required": [
                "logicalOperator"
            ],
            "title": "CompoundSetExpression",
            "type": "object"
        },
        "CompoundSubsetExpression": {
            "additionalProperties": false,
            "description": "",
            "properties": {
                "logicalOperator": {
                    "$ref": "#/$defs/ExpressionLogicalOperator"
                },
                "whereClauses": {
                    "items": {
                        "$ref": "#/$defs/WhereClause"
                    },
                    "type": "array"
                }
            },
            "required": [
                "logicalOperator"
            ],
            "title": "CompoundSubsetExpression",
            "type": "object"
        },
        "ConditionComparator": {
            "description": "",
            "enum": [
                "EQ",
                "NE",
                "GT",
                "GE",
                "LT",
                "LE",
                "IN",
                "NOTIN"
            ],
            "title": "ConditionComparator",
            "type": "string"
        },
        "DataGroup": {
            "additionalProperties": false,
            "description": "A subdivision of the analysis dataset records based on a defined factor.",
            "properties": {
                "compoundExpression": {
                    "$ref": "#/$defs/CompoundGroupExpression"
                },
                "condition": {
                    "$ref": "#/$defs/WhereClauseCondition"
                },
                "id": {
                    "type": "string"
                },
                "label": {
                    "type": "string"
                },
                "level": {
                    "type": "integer"
                },
                "order": {
                    "type": "integer"
                }
            },
            "required": [
                "id"
            ],
            "title": "DataGroup",
            "type": "object"
        },
        "DataGroupingFactor": {
            "additionalProperties": false,
            "description": "A factor used to subdivide data records in an analysis dataset for analysis.",
            "properties": {
                "dataDriven": {
                    "description": "Indicates whether the groups defined by the grouping are prespecified (false) or obtained from distinct data values of the groupingVariable (true).",
                    "type": "boolean"
                },
                "groupingVariable": {
                    "description": "For groupings based on a single variable, a reference to the dataset variable upon which grouping is based.",
                    "type": "string"
                },
                "groups": {
                    "items": {
                        "$ref": "#/$defs/DataGroup"
                    },
                    "type": "array"
                },
                "id": {
                    "type": "string"
                },
                "label": {
                    "type": "string"
                }
            },
            "required": [
                "id",
                "dataDriven"
            ],
            "title": "DataGroupingFactor",
            "type": "object"
        },
        "DataSubset": {
            "additionalProperties": false,
            "description": "A subset of data identified by selection criteria for inclusion in the analysis.",
            "properties": {
                "compoundExpression": {
                    "$ref": "#/$defs/CompoundSubsetExpression"
                },
                "condition": {
                    "$ref": "#/$defs/WhereClauseCondition"
                },
                "id": {
                    "type": "string"
                },
                "label": {
                    "type": "string"
                },
                "level": {
                    "type": "integer"
                },
                "order": {
                    "type": "integer"
                }
            },
            "required": [
                "id"
            ],
            "title": "DataSubset",
            "type": "object"
        },
        "DisplaySection": {
            "additionalProperties": false,
            "description": "",
            "properties": {
                "sectionType": {
                    "$ref": "#/$defs/DisplaySectionType"
                },
                "subSections": {
                    "items": {
                        "$ref": "#/$defs/DisplaySubSection"
                    },
                    "type": "array"
                }
            },
            "title": "DisplaySection",
            "type": "object"
        },
        "DisplaySectionType": {
            "description": "",
            "enum": [
                "Title",
                "Footnote",
                "Abbreviation",
                "Legend",
                "Rowlabel Header"
            ],
            "title": "DisplaySectionType",
            "type": "string"
        },
        "DisplaySubSection": {
            "additionalProperties": false,
            "description": "",
            "properties": {
                "id": {
                    "type": "string"
                },
                "order": {
                    "type": "integer"
                },
                "text": {
                    "type": "string"
                }
            },
            "required": [
                "id",
                "order"
            ],
            "title": "DisplaySubSection",
            "type": "object"
        },
        "DocumentRef": {
            "additionalProperties": false,
            "description": "",
            "properties": {
                "pageRefs": {
                    "items": {
                        "$ref": "#/$defs/PageRef"
                    },
                    "type": "array"
                },
                "referenceDocumentId": {
                    "type": "string"
                }
            },
            "required": [
                "referenceDocumentId"
            ],
            "title": "DocumentRef",
            "type": "object"
        },
        "ExpressionLogicalOperator": {
            "description": "",
            "enum": [
                "AND",
                "OR",
                "NOT"
            ],
            "title": "ExpressionLogicalOperator",
            "type": "string"
        },
        "ExtensibleTerminology": {
            "description": "Extensible ARS enumerations.",
            "enum": [
                "AnalysisReason",
                "AnalysisPurpose",
                "OperationRole",
                "OutputFileType"
            ],
            "title": "ExtensibleTerminology",
            "type": "string"
        },
        "File": {
            "additionalProperties": false,
            "description": "",
            "properties": {
                "fileType": {
                    "anyOf": [
                        {
                            "$ref": "#/$defs/OutputFileType"
                        },
                        {
                            "type": "string"
                        }
                    ]
                },
                "location": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "style": {
                    "type": "string"
                }
            },
            "required": [
                "name"
            ],
            "title": "File",
            "type": "object"
        },
        "Group": {
            "additionalProperties": false,
            "description": "A subdivision of the subject population or analysis dataset record set based on a defined factor.",
            "properties": {
                "compoundExpression": {
                    "$ref": "#/$defs/CompoundGroupExpression"
                },
                "condition": {
                    "$ref": "#/$defs/WhereClauseCondition"
                },
                "id": {
                    "type": "string"
                },
                "label": {
                    "type": "string"
                },
                "level": {
                    "type": "integer"
                },
                "order": {
                    "type": "integer"
                }
            },
            "required": [
                "id"
            ],
            "title": "Group",
            "type": "object"
        },
        "NestedList": {
            "additionalProperties": false,
            "description": "A list of items (analyses or outputs) that may be organized within sub-lists.",
            "properties": {
                "listItems": {
                    "items": {
                        "$ref": "#/$defs/OrderedListItem"
                    },
                    "type": "array"
                }
            },
            "title": "NestedList",
            "type": "object"
        },
        "Operation": {
            "additionalProperties": false,
            "description": "A statistical operation that produces a single analysis result value as part of an analysis method.",
            "properties": {
                "id": {
                    "type": "string"
                },
                "label": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "referencedOperationRelationships": {
                    "items": {
                        "$ref": "#/$defs/ReferencedOperationRelationship"
                    },
                    "type": "array"
                },
                "resultPattern": {
                    "type": "string"
                }
            },
            "required": [
                "id",
                "name"
            ],
            "title": "Operation",
            "type": "object"
        },
        "OperationResult": {
            "additionalProperties": false,
            "description": "The result of an analysis method operation performed on a subdivision of subjects or data records.",
            "properties": {
                "formattedValue": {
                    "type": "string"
                },
                "operationId": {
                    "type": "string"
                },
                "rawValue": {
                    "type": "string"
                },
                "resultGroups": {
                    "items": {
                        "$ref": "#/$defs/ResultGroup"
                    },
                    "type": "array"
                }
            },
            "required": [
                "operationId"
            ],
            "title": "OperationResult",
            "type": "object"
        },
        "OperationRole": {
            "description": "",
            "enum": [
                "NUMERATOR",
                "DENOMINATOR"
            ],
            "title": "OperationRole",
            "type": "string"
        },
        "OrderedDisplay": {
            "additionalProperties": false,
            "description": "",
            "properties": {
                "display": {
                    "$ref": "#/$defs/OutputDisplay"
                },
                "order": {
                    "type": "integer"
                }
            },
            "required": [
                "order"
            ],
            "title": "OrderedDisplay",
            "type": "object"
        },
        "OrderedGroupingFactor": {
            "additionalProperties": false,
            "description": "A reference to a defined factor by which subjects or data records are grouped for the analysis, ordered with respect to other grouping factors.",
            "properties": {
                "groupingId": {
                    "type": "string"
                },
                "order": {
                    "type": "integer"
                },
                "resultsByGroup": {
                    "description": "Indicates whether a result is expected for each group in the grouping.",
                    "type": "boolean"
                }
            },
            "required": [
                "order",
                "resultsByGroup"
            ],
            "title": "OrderedGroupingFactor",
            "type": "object"
        },
        "OrderedListItem": {
            "additionalProperties": false,
            "description": "An item (analysis, output or sub-list) ordered relative to other items within a list or sub-list.",
            "properties": {
                "analysisId": {
                    "type": "string"
                },
                "level": {
                    "type": "integer"
                },
                "name": {
                    "type": "string"
                },
                "order": {
                    "type": "integer"
                },
                "outputId": {
                    "type": "string"
                },
                "sublist": {
                    "$ref": "#/$defs/NestedList"
                }
            },
            "required": [
                "level",
                "order",
                "name"
            ],
            "title": "OrderedListItem",
            "type": "object"
        },
        "Output": {
            "additionalProperties": false,
            "description": "",
            "properties": {
                "categoryIds": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                },
                "displays": {
                    "items": {
                        "$ref": "#/$defs/OrderedDisplay"
                    },
                    "type": "array"
                },
                "fileSpecifications": {
                    "items": {
                        "$ref": "#/$defs/File"
                    },
                    "type": "array"
                },
                "id": {
                    "type": "string"
                },
                "version": {
                    "type": "integer"
                }
            },
            "required": [
                "id"
            ],
            "title": "Output",
            "type": "object"
        },
        "OutputDisplay": {
            "additionalProperties": false,
            "description": "",
            "properties": {
                "displaySections": {
                    "items": {
                        "$ref": "#/$defs/DisplaySection"
                    },
                    "type": "array"
                },
                "displayTitle": {
                    "type": "string"
                },
                "id": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "version": {
                    "type": "integer"
                }
            },
            "required": [
                "id",
                "name"
            ],
            "title": "OutputDisplay",
            "type": "object"
        },
        "OutputFileType": {
            "description": "",
            "enum": [
                "pdf",
                "rtf",
                "txt"
            ],
            "title": "OutputFileType",
            "type": "string"
        },
        "PageNameList": {
            "additionalProperties": false,
            "description": "A list of one or more named document references, each of which corresponds with a page.",
            "properties": {
                "pageNames": {
                    "description": "One or more named document references which each correspond with a page.",
                    "items": {
                        "type": "integer"
                    },
                    "type": "array"
                }
            },
            "required": [
                "pageNames"
            ],
            "title": "PageNameList",
            "type": "object"
        },
        "PageNameRef": {
            "additionalProperties": false,
            "description": "One or more pages in the reference document, referenced by named destination.",
            "properties": {
                "label": {
                    "type": "string"
                },
                "pages": {
                    "$ref": "#/$defs/PageNameList"
                },
                "refType": {
                    "$ref": "#/$defs/PageRefType",
                    "const": "NamedDestination"
                }
            },
            "required": [
                "refType"
            ],
            "title": "PageNameRef",
            "type": "object"
        },
        "PageNumberList": {
            "additionalProperties": false,
            "description": "A list of one or more page numbers.",
            "properties": {
                "pageNumbers": {
                    "description": "One or more page numbers.",
                    "items": {
                        "type": "integer"
                    },
                    "type": "array"
                }
            },
            "required": [
                "pageNumbers"
            ],
            "title": "PageNumberList",
            "type": "object"
        },
        "PageNumberListRef": {
            "additionalProperties": false,
            "description": "One or more individual pages in the reference document, referenced by page number.",
            "properties": {
                "label": {
                    "type": "string"
                },
                "pages": {
                    "$ref": "#/$defs/PageNumberList"
                },
                "refType": {
                    "$ref": "#/$defs/PageRefType",
                    "const": "PhysicalRef"
                }
            },
            "required": [
                "refType"
            ],
            "title": "PageNumberListRef",
            "type": "object"
        },
        "PageNumberRangeRef": {
            "additionalProperties": false,
            "description": "A range of pages in the reference document, indicated by the first and last page number in the range.",
            "properties": {
                "label": {
                    "type": "string"
                },
                "pages": {
                    "$ref": "#/$defs/PageRange"
                },
                "refType": {
                    "$ref": "#/$defs/PageRefType",
                    "const": "PhysicalRef"
                }
            },
            "required": [
                "refType"
            ],
            "title": "PageNumberRangeRef",
            "type": "object"
        },
        "PageRange": {
            "additionalProperties": false,
            "description": "A set of pages in a document that includes the specified first and last page, and all pages in between.",
            "properties": {
                "firstPage": {
                    "description": "The page number of the first page in a range of pages.",
                    "type": "integer"
                },
                "lastPage": {
                    "description": "The page number of the last page in a range of pages.",
                    "type": "integer"
                }
            },
            "required": [
                "firstPage",
                "lastPage"
            ],
            "title": "PageRange",
            "type": "object"
        },
        "PageRef": {
            "additionalProperties": false,
            "description": "",
            "properties": {
                "label": {
                    "type": "string"
                },
                "pages": {
                    "anyOf": [
                        {
                            "$ref": "#/$defs/PageNumberList"
                        },
                        {
                            "$ref": "#/$defs/PageNameList"
                        },
                        {
                            "$ref": "#/$defs/PageRange"
                        }
                    ]
                },
                "refType": {
                    "$ref": "#/$defs/PageRefType"
                }
            },
            "required": [
                "refType"
            ],
            "title": "PageRef",
            "type": "object"
        },
        "PageRefType": {
            "description": "Type of page for page references.",
            "enum": [
                "PhysicalRef",
                "NamedDestination"
            ],
            "title": "PageRefType",
            "type": "string"
        },
        "ProgrammingCodeTemplate": {
            "additionalProperties": false,
            "description": "",
            "properties": {
                "context": {
                    "description": "The name and version of the computer language used for the actual programming statements provided.",
                    "type": "string"
                },
                "parameters": {
                    "description": "Replacement parameters referenced in the programming code template.",
                    "items": {
                        "$ref": "#/$defs/CodeParameter"
                    },
                    "type": "array"
                },
                "templateCode": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "$ref": "#/$defs/DocumentRef"
                        }
                    ],
                    "description": "Template programming statements and/or a reference to the template program used to perform the specific analysis."
                }
            },
            "required": [
                "context"
            ],
            "title": "ProgrammingCodeTemplate",
            "type": "object"
        },
        "ReferenceDocument": {
            "additionalProperties": false,
            "description": "",
            "properties": {
                "id": {
                    "type": "string"
                },
