@prefix NCIT: <http://purl.obolibrary.org/obo/NCIT_> .
@prefix ars: <https://www.cdisc.org/ars/1-0/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ars:NamedObject a sh:NodeShape ;
    sh:closed false ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path ars:name ] ;
    sh:targetClass ars:NamedObject .

ars:PageNameRef a sh:NodeShape ;
    sh:closed true ;
    sh:description "One or more pages in the reference document, referenced by named destination." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class ars:PageNameList ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 2 ;
            sh:path ars:pages ],
        [ sh:description "Alternative label to provide a more specific and description to a page link." ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path ars:label ],
        [ sh:in ( "PhysicalRef" "NamedDestination" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path ars:refType ] ;
    sh:targetClass ars:PageNameRef .

ars:PageNumberListRef a sh:NodeShape ;
    sh:closed true ;
    sh:description "One or more individual pages in the reference document, referenced by page number." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:in ( "PhysicalRef" "NamedDestination" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path ars:refType ],
        [ sh:description "Alternative label to provide a more specific and description to a page link." ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path ars:label ],
        [ sh:class ars:PageNumberList ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 2 ;
            sh:path ars:pages ] ;
    sh:targetClass ars:PageNumberListRef .

ars:PageNumberRangeRef a sh:NodeShape ;
    sh:closed true ;
    sh:description "A range of pages in the reference document, indicated by the first and last page number in the range." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:in ( "PhysicalRef" "NamedDestination" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path ars:refType ],
        [ sh:class ars:PageRange ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 2 ;
            sh:path ars:pages ],
        [ sh:description "Alternative label to provide a more specific and description to a page link." ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path ars:label ] ;
    sh:targetClass ars:PageNumberRangeRef .

ars:ReportingEvent a sh:NodeShape ;
    sh:closed true ;
    sh:description "A set of analyses and outputs created to meet a specific reporting requirement, such as a CSR or interim analysis." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 13 ;
            sh:path ars:name ],
        [ sh:class ars:DisplaySection ;
            sh:nodeKind sh:BlankNode ;
            sh:order 6 ;
            sh:path ars:globalDisplaySections ],
        [ sh:class ars:AnalysisSet ;
            sh:description "The analysis sets (subject populations) defined for the reporting event." ;
            sh:nodeKind sh:IRI ;
            sh:order 2 ;
            sh:path ars:analysisSets ],
        [ sh:class ars:AnalysisCategorization ;
            sh:nodeKind sh:IRI ;
            sh:order 7 ;
            sh:path ars:analysisCategorizations ],
        [ sh:class ars:Output ;
            sh:nodeKind sh:IRI ;
            sh:order 10 ;
            sh:path ars:outputs ],
        [ sh:class ars:AnalysisMethod ;
            sh:description "The defined methods used to analyze any analysis variable." ;
            sh:nodeKind sh:IRI ;
            sh:order 9 ;
            sh:path ars:methods ],
        [ sh:class ars:SubjectGroupingFactor ;
            sh:description "Characteristics used to subdivide the subject population (e.g., treatment, sex, age group)." ;
            sh:nodeKind sh:IRI ;
            sh:order 3 ;
            sh:path ars:analysisGroupings ],
        [ sh:class ars:NestedList ;
            sh:description "An optional structured list of the outputs defined for the reporting event." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 1 ;
            sh:path ars:listOfPlannedOutputs ],
        [ sh:class ars:ReferenceDocument ;
            sh:nodeKind sh:IRI ;
            sh:order 11 ;
            sh:path ars:referenceDocuments ],
        [ sh:class ars:TerminologyExtension ;
            sh:description "Any sponsor-defined extensions to extensible terminology." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 12 ;
            sh:path ars:terminologyExtentions ],
        [ sh:class ars:DataSubset ;
            sh:nodeKind sh:IRI ;
            sh:order 4 ;
            sh:path ars:dataSubsets ],
        [ sh:class ars:Analysis ;
            sh:description "The analyses defined for the reporting event." ;
            sh:nodeKind sh:IRI ;
            sh:order 8 ;
            sh:path ars:analyses ],
        [ sh:class ars:DataGroupingFactor ;
            sh:description "Characteristics used to subdivide data records in the analysis dataset (e.g., visit, system organ class)." ;
            sh:nodeKind sh:IRI ;
            sh:order 5 ;
            sh:path ars:dataGroupings ],
        [ sh:class ars:NestedList ;
            sh:description "A structured list of the analyses defined for the reporting event." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 0 ;
            sh:path ars:listOfPlannedAnalyses ] ;
    sh:targetClass ars:ReportingEvent .

ars:AnalysisGroup a sh:NodeShape ;
    sh:closed true ;
    sh:description "A subdivision of the subject population based on a defined factor (e.g., subjects whose treatment is Drug A, subjects whose gender is male)." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:maxCount 1 ;
            sh:order 3 ;
            sh:path ars:order ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path ars:id ],
        [ sh:class ars:WhereClauseCondition ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 4 ;
            sh:path ars:condition ],
        [ sh:maxCount 1 ;
            sh:order 2 ;
            sh:path ars:level ],
        [ sh:maxCount 1 ;
            sh:order 1 ;
            sh:path ars:label ],
        [ sh:class ars:CompoundGroupExpression ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 5 ;
            sh:path ars:compoundExpression ] ;
    sh:targetClass ars:AnalysisGroup .

ars:AnalysisProgrammingCodeTemplate a sh:NodeShape ;
    sh:closed true ;
    sh:description "Programming statements and/or a reference to a used as a template for creation of a program to perform method operations for a specific analysis." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "The name and version of the computer language used for the actual programming statements provided." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path ars:context ],
        [ sh:description "Programming statements used to perform the specific analysis." ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path ars:code ],
        [ sh:class ars:TemplateCodeParameter ;
            sh:description "Parameters whose values will be used to generate or execute the programming code for a specific analysis." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 3 ;
            sh:path ars:parameters ],
        [ sh:class ars:DocumentRef ;
            sh:nodeKind sh:BlankNode ;
            sh:order 2 ;
            sh:path ars:documentRefs ] ;
    sh:targetClass ars:AnalysisProgrammingCodeTemplate .

ars:CodeParameter a sh:NodeShape ;
    sh:closed true ;
    sh:description "A replacement parameter whose value is substituted in template programming code to create statements required for a specific analysis." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 2 ;
            sh:path ars:name ],
        [ sh:maxCount 1 ;
            sh:order 0 ;
            sh:path ars:description ],
        [ sh:description "The value of the parameter." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path ars:value ] ;
    sh:targetClass ars:CodeParameter .

ars:CompoundSetExpression a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:in ( "AND" "OR" "NOT" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path ars:logicalOperator ],
        [ sh:class ars:AnalysisSet ;
            sh:nodeKind sh:IRI ;
            sh:order 1 ;
            sh:path ars:whereClauses ] ;
    sh:targetClass ars:CompoundSetExpression .

ars:CompoundSubsetExpression a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:in ( "AND" "OR" "NOT" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path ars:logicalOperator ],
        [ sh:class ars:WhereClause ;
            sh:nodeKind sh:BlankNode ;
            sh:order 1 ;
            sh:path ars:whereClauses ] ;
    sh:targetClass ars:CompoundSubsetExpression .

ars:DataGroup a sh:NodeShape ;
    sh:closed true ;
    sh:description "A subdivision of the analysis dataset records based on a defined factor." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class ars:CompoundGroupExpression ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 5 ;
            sh:path ars:compoundExpression ],
        [ sh:class ars:WhereClauseCondition ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 4 ;
            sh:path ars:condition ],
        [ sh:maxCount 1 ;
            sh:order 1 ;
            sh:path ars:label ],
        [ sh:maxCount 1 ;
            sh:order 3 ;
            sh:path ars:order ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path ars:id ],
        [ sh:maxCount 1 ;
            sh:order 2 ;
            sh:path ars:level ] ;
    sh:targetClass ars:DataGroup .

ars:DataGroupingFactor a sh:NodeShape ;
    sh:closed true ;
    sh:description "A factor used to subdivide data records in an analysis dataset for analysis." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Indicates whether the groups defined by the grouping are prespecified (false) or obtained from distinct data values of the groupingVariable (true)." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 3 ;
            sh:path ars:dataDriven ],
        [ sh:description "For groupings based on a single variable, a reference to the dataset variable upon which grouping is based." ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path ars:groupingVariable ],
        [ sh:maxCount 1 ;
            sh:order 1 ;
            sh:path ars:label ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path ars:id ],
        [ sh:class ars:DataGroup ;
            sh:nodeKind sh:IRI ;
            sh:order 4 ;
            sh:path ars:groups ] ;
    sh:targetClass ars:DataGroupingFactor .

ars:DisplaySubSection a sh:NodeShape ;
    sh:closed true ;
    sh:description "An occurrence of a display section containing text that is ordered with respect to other occurrences of the same type of display section." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path ars:order ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path ars:id ],
        [ sh:maxCount 1 ;
            sh:order 2 ;
            sh:path ars:text ] ;
    sh:targetClass ars:DisplaySubSection .

ars:OperationResult a sh:NodeShape ;
    sh:closed true ;
    sh:description "The result of an analysis method operation performed on a subdivision of subjects or data records." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:maxCount 1 ;
            sh:order 2 ;
            sh:path ars:rawValue ],
        [ sh:maxCount 1 ;
            sh:order 3 ;
            sh:path ars:formattedValue ],
        [ sh:class ars:ResultGroup ;
            sh:nodeKind sh:BlankNode ;
            sh:order 1 ;
            sh:path ars:resultGroups ],
        [ sh:class ars:Operation ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path ars:operationId ] ;
    sh:targetClass ars:OperationResult .

ars:OrderedDisplay a sh:NodeShape ;
    sh:closed true ;
    sh:description "A display ordered with respect to other displays in an analysis output." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path ars:order ],
        [ sh:class ars:OutputDisplay ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 1 ;
            sh:path ars:display ] ;
    sh:targetClass ars:OrderedDisplay .

ars:OrderedGroupingFactor a sh:NodeShape ;
    sh:closed true ;
    sh:description "A reference to a defined factor by which subjects or data records are grouped for the analysis, ordered with respect to other grouping factors." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class ars:GroupingFactor ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 1 ;
            sh:path ars:groupingId ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path ars:order ],
        [ sh:description "Indicates whether a result is expected for each group in the grouping." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 2 ;
            sh:path ars:resultsByGroup ] ;
    sh:targetClass ars:OrderedGroupingFactor .

ars:OrderedListItem a sh:NodeShape ;
    sh:closed true ;
    sh:description "An item (analysis, output or sub-list) ordered relative to other items within a list or sub-list." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path ars:order ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path ars:level ],
        [ sh:class ars:Output ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 4 ;
            sh:path ars:outputId ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 5 ;
            sh:path ars:name ],
        [ sh:class ars:NestedList ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 2 ;
            sh:path ars:sublist ],
        [ sh:class ars:Analysis ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 3 ;
            sh:path ars:analysisId ] ;
    sh:targetClass ars:OrderedListItem .

ars:OutputDisplay a sh:NodeShape ;
    sh:closed true ;
    sh:description "A tabular representation of the results of one or more analyses." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 4 ;
            sh:path ars:name ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path ars:id ],
        [ sh:maxCount 1 ;
            sh:order 1 ;
            sh:path ars:version ],
        [ sh:class ars:DisplaySection ;
            sh:nodeKind sh:BlankNode ;
            sh:order 3 ;
            sh:path ars:displaySections ],
        [ sh:maxCount 1 ;
            sh:order 2 ;
            sh:path ars:displayTitle ] ;
    sh:targetClass ars:OutputDisplay .

ars:OutputFile a sh:NodeShape ;
    sh:closed true ;
    sh:description "A file containing analysis output displays." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:maxCount 1 ;
            sh:order 2 ;
            sh:path ars:style ],
        [ sh:maxCount 1 ;
            sh:order 1 ;
            sh:path ars:location ],
        [ sh:maxCount 1 ;
            sh:order 0 ;
            sh:path ars:fileType ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 3 ;
            sh:path ars:name ] ;
    sh:targetClass ars:OutputFile .

ars:PageNameList a sh:NodeShape ;
    sh:closed true ;
    sh:description "A list of one or more named document references, each of which corresponds with a page." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "One or more named document references which each correspond with a page." ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path ars:pageNames ] ;
    sh:targetClass ars:PageNameList .

ars:PageNumberList a sh:NodeShape ;
    sh:closed true ;
    sh:description "A list of one or more page numbers." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "One or more page numbers." ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path ars:pageNumbers ] ;
    sh:targetClass ars:PageNumberList .

ars:PageRange a sh:NodeShape ;
    sh:closed true ;
    sh:description "A set of pages in a document that includes the specified first and last page, and all pages in between." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "The page number of the last page in a range of pages." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path ars:lastPage ],
        [ sh:description "The page number of the first page in a range of pages." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path ars:firstPage ] ;
    sh:targetClass ars:PageRange .

ars:PageRef a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:maxCount 1 ;
            sh:order 2 ;
            sh:path ars:pages ],
        [ sh:description "Alternative label to provide a more specific and description to a page link." ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path ars:label ],
        [ sh:in ( "PhysicalRef" "NamedDestination" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path ars:refType ] ;
    sh:targetClass ars:PageRef .

ars:ReferencedAnalysisOperation a sh:NodeShape ;
    sh:closed true ;
    sh:description "An indication of the analysis that contains results of a referenced operation." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class ars:Analysis ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 1 ;
            sh:path ars:analysisId ],
        [ sh:class ars:ReferencedOperationRelationship ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path ars:referencedOperationId ] ;
    sh:targetClass ars:ReferencedAnalysisOperation .

ars:ResultGroup a sh:NodeShape ;
    sh:closed true ;
    sh:description "For the specified grouping factor, an indication of the specific group of subjects or data records associated with the analysis result." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class ars:GroupingFactor ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path ars:groupingId ],
        [ sh:class ars:Group ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 1 ;
            sh:path ars:groupId ],
        [ sh:maxCount 1 ;
            sh:order 2 ;
            sh:path ars:groupValue ] ;
    sh:targetClass ars:ResultGroup .

ars:SponsorTerm a sh:NodeShape ;
    sh:closed true ;
    sh:description "A sponsor-defined term that is included in an extensible set of controlled terminology." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:maxCount 1 ;
            sh:order 2 ;
            sh:path ars:description ],
        [ sh:description "The specific value expected for submissions." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path ars:submissionValue ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path ars:id ] ;
    sh:targetClass ars:SponsorTerm .

ars:SubjectGroupingFactor a sh:NodeShape ;
    sh:closed true ;
    sh:description "A factor used to subdivide the subject population for comparative analysis (e.g., treatment, sex, race, age)." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class ars:AnalysisGroup ;
            sh:nodeKind sh:IRI ;
            sh:order 4 ;
            sh:path ars:groups ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path ars:id ],
        [ sh:maxCount 1 ;
            sh:order 1 ;
            sh:path ars:label ],
        [ sh:description "For groupings based on a single variable, a reference to the dataset variable upon which grouping is based." ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path ars:groupingVariable ],
        [ sh:description "Indicates whether the groups defined by the grouping are prespecified (false) or obtained from distinct data values of the groupingVariable (true)." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 3 ;
            sh:path ars:dataDriven ] ;
    sh:targetClass ars:SubjectGroupingFactor .

ars:TemplateCodeParameter a sh:NodeShape ;
    sh:closed true ;
    sh:description "A replacement parameter whose value is substituted in template programming code to create statements required for a specific analysis." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 3 ;
            sh:path ars:name ],
        [ sh:maxCount 1 ;
            sh:order 1 ;
            sh:path ars:description ],
        [ sh:description "The value of the parameter." ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path ars:value ],
        [ sh:description "A reference to the prespecified source of the value for the parameter." ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path ars:valueSource ] ;
    sh:targetClass ars:TemplateCodeParameter .

ars:TerminologyExtension a sh:NodeShape ;
    sh:closed true ;
    sh:description "A sponsor-defined term that is included in an extensible set of controlled terminology." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class ars:SponsorTerm ;
            sh:description "The sponsor-defined terms added to the extensible terminology." ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 1 ;
            sh:path ars:sponsorTerms ],
        [ sh:description "The name of the extensible enumeration." ;
            sh:in ( NCIT:C117744 NCIT:C117745 "OperationRole" "OutputFileType" ) ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path ars:enumeration ] ;
    sh:targetClass ars:TerminologyExtension .

ars:WhereClauseCompoundExpression a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:in ( "AND" "OR" "NOT" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path ars:logicalOperator ],
        [ sh:class ars:WhereClause ;
            sh:nodeKind sh:BlankNode ;
            sh:order 1 ;
            sh:path ars:whereClauses ] ;
    sh:targetClass ars:WhereClauseCompoundExpression .

ars:AnalysisCategorization a sh:NodeShape ;
    sh:closed true ;
    sh:description "A set of related implementer-defined categories that can be used to categorize analyses or outputs." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path ars:id ],
        [ sh:maxCount 1 ;
            sh:order 1 ;
            sh:path ars:label ],
        [ sh:class ars:AnalysisCategory ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 2 ;
            sh:path ars:categories ] ;
    sh:targetClass ars:AnalysisCategorization .

ars:AnalysisMethod a sh:NodeShape ;
    sh:closed true ;
    sh:description "A set of one or more statistical operations." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path ars:id ],
        [ sh:maxCount 1 ;
            sh:order 2 ;
            sh:path ars:description ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 6 ;
            sh:path ars:name ],
        [ sh:maxCount 1 ;
            sh:order 1 ;
            sh:path ars:label ],
        [ sh:class ars:Operation ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 3 ;
            sh:path ars:operations ],
        [ sh:class ars:DocumentRef ;
            sh:nodeKind sh:BlankNode ;
            sh:order 4 ;
            sh:path ars:documentRefs ],
        [ sh:class ars:AnalysisProgrammingCodeTemplate ;
            sh:description "Template programming statements and/or a reference to the template program used to perform the specific analysis." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 5 ;
            sh:path ars:codeTemplate ] ;
