ars:Analysis a sh:NodeShape ;
    sh:closed true ;
    sh:description """An analysis that is designed to meet a requirement of the reporting event. Each analysis is defined as a set of specifications, including:
* The analysis variable that is the subject of the analysis.
* The analysis method (set of statistical operations) that is performed for the analysis variable.
* The analysis set (subject population) for which the analysis is performed.
* The subset of data records on which the analysis is performed (optional).
* One or more factors by which either subjects or data records are grouped for analysis (optional).
""" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class ars:ReferencedAnalysisOperation ;
            sh:nodeKind sh:BlankNode ;
            sh:order 13 ;
            sh:path ars:referencedAnalysisOperations ],
        [ sh:class ars:AnalysisOutputProgrammingCode ;
            sh:description "Programming statements and/or a reference to the program used to perform the specific analysis." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 14 ;
            sh:path ars:programmingCode ],
        [ sh:class ars:DataSubset ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 9 ;
            sh:path ars:dataSubsetId ],
        [ sh:maxCount 1 ;
            sh:order 10 ;
            sh:path ars:dataset ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path ars:id ],
        [ sh:class ars:DocumentRef ;
            sh:nodeKind sh:BlankNode ;
            sh:order 6 ;
            sh:path ars:documentRefs ],
        [ sh:class ars:AnalysisCategory ;
            sh:nodeKind sh:IRI ;
            sh:order 2 ;
            sh:path ars:categoryIds ],
        [ sh:class ars:OrderedGroupingFactor ;
            sh:nodeKind sh:BlankNode ;
            sh:order 8 ;
            sh:path ars:orderedGroupings ],
        [ sh:maxCount 1 ;
            sh:order 1 ;
            sh:path ars:version ],
        [ sh:description "The purpose of the analysis within the body of evidence (e.g., section in the clinical study report)." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 5 ;
            sh:path ars:purpose ],
        [ sh:class ars:AnalysisSet ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 7 ;
            sh:path ars:analysisSetId ],
        [ sh:class ars:AnalysisMethod ;
            sh:description "A reference to the set of one or more statistical operations performed for the analysis." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 12 ;
            sh:path ars:methodId ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 16 ;
            sh:path ars:name ],
        [ sh:maxCount 1 ;
            sh:order 3 ;
            sh:path ars:description ],
        [ sh:description "The rationale for performing this analysis. It indicates when the analysis was planned." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 4 ;
            sh:path ars:reason ],
        [ sh:class ars:OperationResult ;
            sh:nodeKind sh:BlankNode ;
            sh:order 15 ;
            sh:path ars:results ],
        [ sh:maxCount 1 ;
            sh:order 11 ;
            sh:path ars:variable ] ;
    sh:targetClass ars:Analysis .

